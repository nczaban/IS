@book{sega07,
    Address = {Sebastapol, CA},
	Author = {Toby ~Segaran},
	Publisher = {O'Reilly},
	Title = {Programming Collective Intelligence},
	Year = 2007,
	Annotate = {In this book, Segaran introduces a number of instances where collective intelligence, or the combination and analysis of information from various sources, and introduces an algorithm that can be used to improve this scenario. For example, a website may use decision trees to predict which users will become paying customers after signing up for a free trial. The author goes on to describe how to implement these algorithms in Python. Many of these algorithms are not useful in creating a video game, but the sections on decision trees and possibly the section on genetic algorithms will be helpful in developing the AI of the game enemies.}}

@book{shoh09,
    Address = {New York},
	Author = {Yoav ~Shoham and Kevin ~Leyton-Brown},
	Publisher = {Cambridge University},
	Title = {Multiagent Systems},
    Year = 2009,
	Annotate = {This book is designed as an introduction to the field of multiagent systems, a group of multiple autonomous entities with different interests or information. These systems are built heavily upon game theory. Shoham and Leyton-Brown provide several formal definitions of game theory topics, while still providing excellent examples and easy-to-understand descriptions. The section on extensive-form game trees, in particular, is extremely useful. Their explanations of how to model sequential games and use the models to calculate equilibria are superb.}}

@book{smed06,
    Author = {Jouni ~Smed and Harri ~Hakonen},
	Publisher = {John Wiley \& Sons},
	Address = {West Sussex, England},
	Title = {Algorithms and Networking for Computer Games},
	Year = 2006,
	Annotate = {In their book, Smed and Hakonen look at various computer algorithms that are essential to creating a computer game. The book also goes into detail about the computer networking knowledge needed to implement online multiplayer in a computer game. While the book does contain some excellent pseudocode for each algorithm, the mathematics behind each algorithm is largely ignored. However, this text does a fantastic job of explaining each concept in plain English, and provides some clear examples of games utilizing each concept. Among the algorithms described by the authors, the most useful to my project will be the game trees, decision making, and fuzzy sets/constraint problems.}}

@book{rabi10,
    Editor = {Steve ~Rabin},
	Publisher = {Charles River Media},
	Address = {Boston, MA},
	Title = {Introduction to Game Development},
	Edition = {2nd},
	Year = 2010,
	Annotate = {This book provides a comprehensive look at game development from design and programming to the business side. Each section is written by an expert in their field, combining over 20 professional game developers in one volume. The design chapter looks at the psychological aspects that a designer can use to ellicit their desired emotions from players, as well as ways that make the game intuitive. The programming section details various programming paradigms, the difference between game-specific and game-engine code, and algorithms for collision detection, physics simulations, and the graphics pipeline. A section on artificial intelligence mentions various different techniques, but offers little substantive on how to implement them; there are, however, extensive citations that may be of more use.}}

@article{zade65,
    Author = {Lofti ~Zadeh},
	Title = {Fuzzy Sets},
	Journal = {Information and Control},
	Volume = 8,
	Pages = {338-353},
	Year = 1965,
	Annotate = {This article is the original publication discussing fuzzy sets, or sets without rigid definition membership. As the first in its field, it contains thorough mathematical definitions of fuzzy sets. Zadeh goes on to define how various functions and properties, such as union, algebraic product, and convexity, operate on fuzzy sets. Each theorem is accompanied by a proof; these proofs are clear and concise. Since the entire field of fuzzy sets is built upon this paper, this source will be crucial for understanding fuzzy logic and fuzzy-state machines.}}

