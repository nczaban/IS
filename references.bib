@article{sola15,
    Author = {Eilon ~Solan and Nicolas Vieille},
	Title = {Stochastic games},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Volume = 112,
	Issue = 45,
	Pages = {13743-13746},
    Year = 2015,
	Annotate = {This article focuses on the history and importance of Lloyd Shapley's paper on stochastic games. Stochastic games are those where a particular choice may have multiple, sometimes contradictory, effects. This type of decision making and decision balancing means that players must consider future payoffs when making choices in the game. One major area of stochastic games are repeated games or those with imperfect information. Players must make their decisions based on their privately-held information, and their lack of knowledge may lead to them making a bad decision. This article was particularly lacking an in-depth discussion of the mathematics, but there are a number of sources cited in this paper which may contain a more robust definition. Stochastic games will likely be relevant when fuzzy logic is implemented into the game, since it may not be clear in which fuzzy set a player is included, thus limiting the information of the game.}}

@book{mail06,
    Author = {George J. ~Mailath and Larry ~Samuelson},
	Title = {Repeated Games and Reputations},
	Publisher = {Oxford University},
	Year = 2006,
	Annotate = {In their book, Mailath and Samuelson describe games with "intertemporal incentives," or the idea of value persisting between one iteration of a game and the next. For example, a player who bluffs in a card game will be less successful in future games if other players learn they are bluffing. The text looks at three different types of repeated games, separated by types of monitoring: perfect, imperfect public, and private monitoring. The authors also look at the formation of a player's reputation, both in the long-term and short-term. While designing my game, these concepts could be incorporated as a type of difficulty curving. Players who are doing well will have notoriety with their enemies and attract stronger challengers, while players who are doing poorly will attract overconfident opponents.}}

@book{sche15,
    Address = {Pittsburg, PA},
	Author = {Jesse ~Schell},
	Publisher = {CRC Press},
	Title = {The Art of Game Design},
	Year = 2015,
	Annotate = {Jesse Schell provides a in-depth look at the process of game design, building up from a general experience the designer wishes to create toward a rich, intricate world that satisfies all requirements given to the designer. The text is very accessible, with very little math or programming knowledge required. Instead, its purpose is to focus on the creative process of game design, a purpose which will be useful to keep in mind when designing the final software. Aside from the theory and algorithms, the game should still be enjoyable.}}

@book{schw04,
    Address = {Hingham, MA},
	Author = {Brian ~Schwab},
	Publisher = {Charles River Media},
	Title = {AI Game Engine Programming},
	Year = 2004,
	Annotate = {Brian Schwab outlines a basic model of AI for a number of video game genres in his book. Schwab spends a good deal of time tying the individual elements that make up a typical game of the genre to the type and amound of AI these elements need or can use. The book also touches on fuzzy-state machines, which is a superset of logic capable of handling partial truths. The fuzzy set chapter will be very useful in expanding the AI of the game to understand healthy or unhealthy states. There is also code snippets both from actual RPGs and for a fuzzy-state machine class, both of which will be invaluable resources for the software component of my IS.}}

